mat1=randi(10,3,4); mat2=randi(10,3,4);
mat_result = zeros(3,4);

Nrows=3; Ncolumns=4;

for i=1:Nrows
    for j=1:Ncolumns
        mat_result(i,j)=mat1(i,j).*mat2(i,j);
        fprintf('%2.0d ', mat_result(i,j));
    end
    fprintf('\n')
end

% print check test

disp(mat1); disp(mat2); disp(mat_result); 